{
	"regions": ["gru1"],
	"version": 2,
	"buildCommand": "npx prisma generate && npm run build",
	"outputDirectory": "dist",
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": ["GET", "POST", "PUT", "DELETE"]
		}
	]
}
